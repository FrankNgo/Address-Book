<h1>Address Book</h1>
<ul>
  @foreach (var contact in Model)
  {
    <li><a href='/contacts/@contact.GetId()'>@contact.GetName()</a></li>
  }
</ul>
<a href='/contacts/new'>Add new Contact</a>
<form action="/contacts/list/clear" method="post">
  <button type="submit" name="button">Clear All Contact</button>
</form>
